/* 导航栏 */
.div {
	box-sizing: initial;
}
.navBar {
	z-index: 10;
	padding-top: 20px;
	display: flex;
	width: 100%;
	align-items: center;
	justify-content: flex-start;
	height: 44px;
}
.navBar.fixed {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
}
.navBar .back {
	display: flex;
	justify-content: center;
	align-items: center;
	margin-left: 20rpx;
	width: 64rpx;
	height: 64rpx;
	background-color: rgba(255, 255, 255, 0.6);
	border-radius: 50%;
	color: #000;
}
.navBar .back .img {
	width: 34rpx;
	height: 34rpx;
}
